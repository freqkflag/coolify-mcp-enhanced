{
  "name": "@freqkflag/coolify-mcp-enhanced",
  "scope": "@freqkflag",
  "version": "0.2.0",
  "description": "Enhanced MCP server for comprehensive Coolify infrastructure management with 50+ tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "bin": {
    "coolify-mcp-enhanced": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc && shx chmod +x dist/*.js",
    "dev": "tsc --watch",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "prepublishOnly": "npm test && npm run lint",
    "start": "node dist/index.js",
    "docker:build": "docker build -t freqkflag/coolify-mcp-enhanced .",
    "docker:run": "docker run -d --name coolify-mcp -e COOLIFY_BASE_URL= -e COOLIFY_ACCESS_TOKEN= freqkflag/coolify-mcp-enhanced"
  },
  "keywords": [
    "coolify",
    "mcp",
    "model-context-protocol",
    "deployment",
    "docker",
    "infrastructure",
    "devops",
    "ai",
    "automation",
    "self-hosted",
    "applications",
    "databases",
    "services",
    "domains",
    "ssl",
    "backups",
    "monitoring"
  ],
  "author": {
    "name": "freqkflag",
    "email": "freqkflag@example.com",
    "url": "https://github.com/freqkflag"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/freqkflag/coolify-mcp-enhanced.git"
  },
  "bugs": {
    "url": "https://github.com/freqkflag/coolify-mcp-enhanced/issues"
  },
  "homepage": "https://github.com/freqkflag/coolify-mcp-enhanced#readme",
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/freqkflag"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.6.1",
    "debug": "^4.3.4",
    "reflect-metadata": "^0.2.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.23",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "markdownlint-cli2": "^0.12.1",
    "prettier": "^3.5.3",
    "shx": "^0.3.4",
    "ts-jest": "^29.2.6",
    "typescript": "^5.8.2"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}